{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "learning react and express",
  "main": "index.js",
  "author": "Rodney Matambo",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel webpack:*",
    "webpack:server": "webpack --config webpack.server.js --watch",
    "webpack:client": "webpack --config webpack.client.js --watch",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "build": "webpack --config webpack.client.js && webpack --config webpack.server.js "
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.5.3",
    "cors": "^2.8.5",
    "css-loader": "^5.0.1",
    "express": "^4.17.1",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^6.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "mini-css-extract-plugin": "^1.3.3"
  }
}
